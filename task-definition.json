{
  "containerDefinitions": [
    {
      "name": "billingwise-container",
      "image": "901929896752.dkr.ecr.ap-northeast-2.amazonaws.com/billingwise-api-ecr",
      "cpu": 0,
      "portMappings": [
        {
          "name": "billingwise-api-port",
          "containerPort": 8081,
          "hostPort": 8081,
          "protocol": "tcp",
          "appProtocol": "http"
        }
      ],
      "essential": true,
      "environment": [],
      "environmentFiles": [],
      "mountPoints": [],
      "volumesFrom": [],
      "ulimits": [],
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "billingwise-api-log",
          "awslogs-create-group": "true",
          "awslogs-region": "ap-northeast-2",
          "awslogs-stream-prefix": "ecs"
        },
        "secretOptions": []
      },
      "systemControls": []
    }
  ],
  "family": "billingwise-task",
  "taskRoleArn": "arn:aws:iam::901929896752:role/ecsTaskExecutionRole",
  "executionRoleArn": "arn:aws:iam::901929896752:role/ecsTaskExecutionRole",
  "networkMode": "awsvpc",
  "volumes": [],
  "placementConstraints": [],
  "requiresCompatibilities": [
    "FARGATE"
  ],
  "cpu": "1024",
  "memory": "3072",
  "runtimePlatform": {
    "cpuArchitecture": "X86_64",
    "operatingSystemFamily": "LINUX"
  },
  "tags": []
}